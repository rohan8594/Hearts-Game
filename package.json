{
  "name": "term-project-hearts-game",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "NODE_ENV=development DEBUG=APP_NAME:* nodemon ./bin/www",
    "db:create:migration": "sequelize migration:generate --name ",
    "db:migrate": "sequelize db:migrate",
    "db:rollback": "sequelize db:migrate:undo",
    "postinstall": "sequelize db:migrate",
    "db:create:seed": "sequelize seed:generate --name ",
    "db:seed:all": "sequelize db:seed:all",
    "db:seed": "sequelize db:seed --seed ",
    "db:seed:undo": "sequelize db:seed:undo:all"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.20.1",
    "clean-css": "^4.2.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.1.0",
    "event-stream": "^3.3.4",
    "express": "~4.18.2",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg-promise": "^8.5.0",
    "pug": "^2.0.4",
    "save": "^2.4.0",
    "sequelize": "^5.21.1",
    "sequelize-cli": "^5.5.1",
    "set-value": "^3.0.1",
    "socket.io": "^2.1.1"
  },
  "devDependencies": {
    "nodemon": "^1.18.4"
  }
}
